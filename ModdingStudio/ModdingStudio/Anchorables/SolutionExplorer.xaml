<ad:LayoutAnchorable x:Class="ModdingStudio.Anchorables.SolutionExplorer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:ad="clr-namespace:Xceed.Wpf.AvalonDock.Layout;assembly=Xceed.Wpf.AvalonDock"
             xmlns:msd="clr-namespace:ModdingStudio.Documents"
             xmlns:msa="clr-namespace:ModdingStudio.Anchorables"
             mc:Ignorable="d">
    <Grid>
        <Border BorderBrush="#FFcccedb" BorderThickness="1">
            <Grid Background="#FFf5f5f5">
                <Grid.RowDefinitions>
                    <RowDefinition Height="4"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <msa:SolutionExplorerTreeView x:Name="treeView" Grid.Row="1">

                </msa:SolutionExplorerTreeView>
            </Grid>
        </Border>
    </Grid>
</ad:LayoutAnchorable>
